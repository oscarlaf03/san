<p id="notice"><%= notice %></p>
  <h1>Beneficiarios</h1>

  <table class='ui celled table'>
    <thead>
      <tr>
        <th>nome</th>
        <th>cpf</th>
        <th>email</th>
        <th colspan="3">Ações</th>
      </tr>
    </thead>

    <tbody>
      <% @beneficiarios.each do |beneficiario| %>
        <tr>
          <td><%= link_to beneficiario.nome, beneficiario %></td>
          <td><%= beneficiario.cpf %></td>
          <td><%= beneficiario.email %></td>
          <% edit_button= '<div class="ui icon button blue" data-tooltip="Editar"><i class="edit outline icon"></i></div>'.html_safe%>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>
