<div class="card organizacao-info-card">
  <div class="model-card">
    <div class="card-body">
    <h5 class='card-title'>
        <%= @organizacao.nome_fantasia %>
      </h5>
      <div class="card-text">  
        <p>
          <strong>Razao social:</strong>
          <%= @organizacao.razao_social %>
        </p>
        <p>
          <strong>Cnpj:</strong>
          <%= @organizacao.cnpj %>
        </p>
        <p>
          <strong>Inscrição Municipal:</strong>
          <%= @organizacao.inscricao_municipal %>
        </p>
        <p>
          <strong>Inscrição Estadual:</strong>
          <%= @organizacao.inscricao_estadual %>
        </p>
        <%= link_to edit_button, edit_organizacao_path(@organizacao) %>
      </div>
    </div>

  </div>
</div>
